{
  "manifest_version": 3,
  "name": "Tab Organizer",
  "description": "Manages tabs into predefined tabGroups",
  "version": "1.1",
  "icons": {
    "16": "images/tabs-icon-16.png",
    "32": "images/tabs-icon-32.png",
    "48": "images/tabs-icon-48.png",
    "128": "images/tabs-icon-128.png"
  },
  "action": {
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "tabs",
    "tabGroups"
  ],
  "background": {
    "service_worker": "background.js"
  }
}